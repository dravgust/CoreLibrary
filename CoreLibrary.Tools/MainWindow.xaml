        <Label Content="Suppliers" HorizontalAlignment="Left" Margin="626,14,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextBoxSuppliers" HorizontalAlignment="Left" Margin="693,20,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="67"/>
        <TextBox x:Name="TextBoxOutput" AcceptsReturn="True" HorizontalAlignment="Left" Margin="10,18,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="602" Height="367" HorizontalScrollBarVisibility="Auto"/>

    </Grid>
</Window>
